# DeepAgents Environment Configuration

# Feature Flags
# Enable interrupt pattern for ask_user_question (Phase 2)
USE_INTERRUPT_PATTERN=false

# Checkpointing Configuration
# Set to true to disable checkpointing entirely
DISABLE_CHECKPOINTING=false

# SQLite checkpointer configuration (development)
SQLITE_DB_PATH=deepagents.db

# PostgreSQL checkpointer configuration (production)
# POSTGRES_URL=postgresql://user:password@localhost:5432/deepagents

# API Keys (required)
OPENAI_API_KEY=your_openai_api_key_here
# Or use other providers:
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GROQ_API_KEY=your_groq_api_key_here

# Frontend Configuration
# For the UI to use interrupt pattern
NEXT_PUBLIC_USE_INTERRUPT_PATTERN=false
NEXT_PUBLIC_DEPLOYMENT_URL=http://127.0.0.1:2024
NEXT_PUBLIC_AGENT_ID=research